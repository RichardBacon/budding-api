  "bugs": {
    "url": "https://github.com/richardjohnbacon/budding-back-end/issues"
  },
  "homepage": "https://github.com/richardjohnbacon/budding-back-end#readme",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "jest-extended": "^0.11.5",
    "jest-sorted": "^1.0.6",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.21.1",
    "pg": "^8.2.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended",
      "jest-sorted"
    ]
  }
}
